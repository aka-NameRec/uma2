# PostgreSQL connection string (asyncpg required)
# Example: postgresql+asyncpg://user:password@localhost:5432/database_name
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/uma_demo

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Debug mode: if true, error responses will include detailed traceback
# This affects ONLY error response details, not log verbosity
DEBUG_MODE=false
